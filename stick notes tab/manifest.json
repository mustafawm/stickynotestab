{
    "manifest_version": 2,
    "name": "Sticky Notes Tab",
    "short_name": "Stick Notes Tab",
    "version": "1.2.1",
    "description": "Simple sticky notes board - tab extension",
    "author": "Mustafa Al-Saegh",
    "homepage_url": "http://mustafa-watha.com",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "sticky_notes.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}